{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": ["references_text", "confidence", "notes"],
  "properties": {
    "references_text": {"type": ["string", "null"]},
    "confidence": {"type": "number", "minimum": 0, "maximum": 1},
    "notes": {
      "type": "array",
      "items": {"type": "string"}
    }
  }
}
